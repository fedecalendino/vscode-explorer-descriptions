{
	"name": "fsdocs",
	"displayName": "File Structure Docs",
	"description": "Allow to document the contents of specific files and folders on your project.",
	"icon": "images/icon.png",
	"version": "0.7.1",
	"publisher": "fedecalendino",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/fedecalendino/vscode-fsdocs"
	},
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:fsdocs-file-explorer",
		"onLanguage:json",
		"onLanguage:jsonc",
		"onCommand:testView.reveal"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "fsdocs-activity-bar",
					"title": "File Structure Docs",
					"icon": "$(book)"
				}
			]
		},
		"views": {
			"fsdocs-activity-bar": [
				{
					"id": "fsdocs-file-explorer",
					"name": "File Structure Docs"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "fsdocs-file-explorer.open-config-file",
					"group": "navigation",
					"icon": "$(edit)",
					"when": "view == fsdocs-file-explorer"
				},
				{
					"command": "fsdocs-file-explorer.search-element",
					"group": "navigation",
					"when": "view == fsdocs-file-explorer"
				},
				{
					"command": "fsdocs-file-explorer.refresh-tree",
					"group": "navigation",
					"when": "view == fsdocs-file-explorer"
				}
			],
			"view/item/context": [
				{
					"command": "fsdocs-file-explorer.reveal-element",
					"group": "inline",
					"when": "view == fsdocs-file-explorer"
				},
				{
					"command": "fsdocs-file-explorer.copy-element-label",
					"group": "5_copypaste",
					"when": "view == fsdocs-file-explorer"
				},
				{
					"command": "fsdocs-file-explorer.copy-element-name",
					"group": "5_copypaste",
					"when": "view == fsdocs-file-explorer"
				},
				{
					"command": "fsdocs-file-explorer.copy-element-path",
					"group": "5_copypaste",
					"when": "view == fsdocs-file-explorer"
				}
			]
		},
		"commands": [
			{
				"command": "fsdocs-file-explorer.open-file",
				"title": "Open file"
			},
			{
				"command": "fsdocs-file-explorer.copy-element-label",
				"title": "Copy label"
			},
			{
				"command": "fsdocs-file-explorer.copy-element-name",
				"title": "Copy name"
			},
			{
				"command": "fsdocs-file-explorer.copy-element-path",
				"title": "Copy path"
			},
			{
				"command": "fsdocs-file-explorer.reveal-element",
				"title": "Reveal in editor"
			},
			{
				"command": "fsdocs-file-explorer.open-config-file",
				"title": "Open config file",
				"icon": "$(pencil)"
			},
			{
				"command": "fsdocs-file-explorer.refresh-tree",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "fsdocs-file-explorer.search-element",
				"title": "Search",
				"icon": "$(search)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^16.11.7",
		"@types/rimraf": "^2.0.2",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.13.0",
		"typescript": "^4.7.2"
	},
	"dependencies": {
		"ftp": "^0.3.10",
		"jsonc-parser": "^0.4.2",
		"minimist": "^1.2.6",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2"
	}
}
